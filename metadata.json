{
  "name": "puppet-vsftpd",
  "version": "0.3.1",
  "author": "Matthias Saou, Adam Brust",
  "summary": "Module to install, configure and manage vsftpd",
  "license": "Apache-2.0",
  "source": "https://github.com/UCSD-ANF/puppet-vsftpd",
  "project_page": "https://github.com/UCSD-ANF/puppet-vsftpd",
  "issues_url": "https://github.com/UCSD-ANF/puppet-vsftpd/issues",
